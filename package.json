    "babel-jest": "^21.2.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-assign": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "compare-versions": "^3.1.0",
    "create-react-class": "^15.6.2",
    "jest-cli": "^21.2.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
